import ValueObject from './__TEMPLATE__FILE__NAME__';

describe('value-object', () => {
	describe('success cases', () => {

		it('should create a valid instance', () => {
			const result = ValueObject.create('valid_value');
			expect(result.isOk()).toBeTruthy();
		});
	});

	describe('failure cases', () => {

		it('should fail if provide undefined value', () => {
			const result = ValueObject.create(undefined as unknown as string);
			expect(result.isFail()).toBeTruthy();
		});
	});
	
});
